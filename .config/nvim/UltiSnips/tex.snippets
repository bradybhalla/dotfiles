extends pset

snippet no "No indent"
\noindent 
endsnippet

snippet beg "Begin environment"
\begin{$1}
	$0
\end{$1}
endsnippet

snippet ali "Align environment"
\begin{align*}
	$0
\end{align*}
endsnippet

snippet "(\S+)/(\S+)" "Expand fraction" r
\frac{`!p snip.rv=match.group(1)`}{`!p snip.rv=match.group(2)`} 
endsnippet
